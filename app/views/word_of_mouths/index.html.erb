<p id="notice"><%= notice %></p>

<h1>Word Of Mouths</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @word_of_mouths.each do |word_of_mouth| %>
      <tr>
        <td><%= link_to 'Show', word_of_mouth %></td>
        <td><%= link_to 'Edit', edit_word_of_mouth_path(word_of_mouth) %></td>
        <td><%= link_to 'Destroy', word_of_mouth, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Word Of Mouth', new_word_of_mouth_path %>
